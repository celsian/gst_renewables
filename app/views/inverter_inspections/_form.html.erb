<%= form_for @inverter_inspection do |f| %>
  <%= f.label :name %><%= f.text_field :name, placeholder: "Inverter Name" %>
          <%= f.fields_for :infoboxes do |builder| %>
            <%= render "infobox_fields", :f => builder %>
          <% end %>
          <P>
          <%= link_to_add_fields "Add Inspection", f, :infoboxes %>
          </p>
          <%= f.hidden_field :pv_commission_id, value: params[:pv_commission_id] ||= @inverter_inspection.pv_commission_id %>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>