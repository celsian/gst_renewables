<%= form_for @pv_array_test, multipart: true do |f| %>
    <%= f.label :combiner_exterior_pic %><%= f.file_field :combiner_exterior_pic %>
    <%= f.label :combiner_interior_pic %><%= f.file_field :combiner_interior_pic %> 
    <%= f.label :combiner_flir_pic %><%= f.file_field :combiner_flir_pic %> 
    <% if params[:pv_commission_id] %><%= f.hidden_field :pv_commission_id, value: params[:pv_commission_id] %><% end %>
  <br>
  <%= f.submit %>
<% end %>