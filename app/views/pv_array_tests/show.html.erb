<div class="row">
  <span class="span7">
    <H2>PV Array Test: <%= @pv_array_test.test_number %></h2>
  </span>
  <span class="span2">
    <p class="text-right">
      <button class="btn btn-small btn-primary" type="button">
        <%= link_to "Edit", edit_pv_array_test_path(@pv_array_test), {style: 'color:#FFFFFF;'} %>
      </button>
      <button class="btn btn-small btn-danger" type="button">
        <%= link_to "Delete", @pv_array_test, data: { confirm: "Are you sure you want to permanently delete this PV Array Test?" }, style: 'color:#FFFFFF;', method: :delete %>
      </button>
    </p>
  </span>
</div>

<table class="table table-striped" width=75%>
  <tr>
    <td width=30%><B>Inspector:</B></td><td width=70%><%= @pv_array_test.inspector %></td>
  </tr>
  <tr>
    <td width=30%><B>Verification:</B></td><td width=70%><%= @pv_array_test.initial_verification ? "Initial" : "Periodic" %></td>
  </tr>
  <tr>
    <td width=30%><B>Test Instruments:</B></td><td width=70%><%= @pv_array_test.test_instruments %></td>
  </tr>
  <tr>
    <td width=30%><B>Combiner:</B></td><td width=70%><%= @pv_array_test.combiner %></td>
  </tr>
  <tr>
    <td width=30%><B>MFG:</B></td><td width=70%><%= @pv_array_test.mfg %></td>
  </tr>
</table>

<table class="table table-striped">
  <tr>
    <th class="c">Combiner Exterior Picture</th><th class="c">Combiner Interior Picture</th><th class="c">Combiner FLIR Picture</th>
  </tr>
  <tr>
    <td class="c">
  <%= link_to image_tag(@pv_array_test.combiner_exterior_pic_url(:thumb).to_s), @pv_array_test.combiner_exterior_pic_url.to_s %>
    </td>
    <td class="c">
  <%= link_to image_tag(@pv_array_test.combiner_interior_pic_url(:thumb).to_s), @pv_array_test.combiner_interior_pic_url.to_s %>
    </td>
    <td class="c">
  <%= link_to image_tag(@pv_array_test.combiner_flir_pic_url(:thumb).to_s), @pv_array_test.combiner_flir_pic_url.to_s %>
    </td>
  </tr>
</table>

<table class="table table-striped" border=1>
  <tr>
    <th width=50px>String ID:</th><td></td>
  </tr>
  <tr>
    <th width=50px>Array:</th>
    <td>
      <table class="table table-striped">
        <tr>
          <td>Module</td>
        </tr>
        <tr>
          <td>Quantity</td>
        </tr>
        <tr>
          <td>Parameters VOC</td>
        </tr>
        <tr>
          <td>Parameters ISC</td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <th width=50px>String:</th><td><table class="table table-striped"><tr><td>Type</td></tr><tr><td>Rating</td></tr><tr><td>D Rating</td></tr><tr><td>Capacity</td></tr></table></td>
  </tr>
  <tr>
    <th width=50px>Wiring:</th><td><table class="table table-striped"><tr><td>Type</td></tr><tr><td>Insulation</td></tr><tr><td>Size</td></tr></table></td>
  </tr>
  <tr>
    <th width=50px>String Test:</th><td><table class="table table-striped"><tr><td>VOC</td></tr><tr><td>ISC</td></tr><tr><td>Irradiance</td></tr></table></td>
  </tr>
  <tr>
    <th width=50px>Array Insulation Resistance:</th><td><table class="table table-striped"><tr><td>Test Voltage</td></tr><tr><td>POS</td></tr><tr><td>NEG</td></tr></table></td>
  </tr>
  <tr>
    <th width=50px>Array FLIR:</th><td></td>
  </tr>
</table>

